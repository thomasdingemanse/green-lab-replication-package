  <div class="adobe-fonts-filtered-family-list__pagination">
    <nav class="spectrum-Pagination spectrum-Pagination--listing spectrum-grid-center-xs">
      <a class="spectrum-Button spectrum-Button--primary spectrum-Button--quiet spectrum-Pagination-prevButton tk-paginate-prev-button"
          ng-click="tkPaginate.previousPage()"
          ng-if="!tkPaginate.isSelected(1)"> Prev </a>
      <span ng-if="tkPaginate.isSelected(1)" class="tk-paginate-disabled"> Prev </span>
      <a class="spectrum-ActionButton spectrum-ActionButton--quiet tk-paginate-action-button"
          ng-class="{'is-selected' : tkPaginate.isSelected(1)}"
          ng-click="tkPaginate.selectPage(1)">1</a>
      <a class="spectrum-ActionButton spectrum-ActionButton--quiet tk-paginate-action-button" ng-if="tkPaginate.showBeginEllipsis()">...</a>
      <a class="spectrum-ActionButton spectrum-ActionButton--quiet tk-paginate-action-button"
          ng-repeat="page in tkPaginate.getPagesInRange()"
          ng-class="{'is-selected' : tkPaginate.isSelected(page)}"
          ng-click="tkPaginate.selectPage(page)">{{page}}</a>
      <a class="spectrum-ActionButton spectrum-ActionButton--quiet tk-paginate-action-button" ng-if="tkPaginate.showEndEllipsis()">...</a>
      <a class="spectrum-ActionButton spectrum-ActionButton--quiet tk-paginate-action-button"
          ng-if="tkPaginate.totalPages > 1"
          ng-class="{'is-selected' : tkPaginate.isSelected(tkPaginate.totalPages)}"
          ng-click="tkPaginate.selectPage(tkPaginate.totalPages)">{{tkPaginate.totalPages}}</a>
      <a class="spectrum-Button spectrum-Button--primary spectrum-Button--quiet spectrum-Pagination-nextButton tk-paginate-next-button"
          ng-click="tkPaginate.nextPage()"
          ng-if="!tkPaginate.isSelected(tkPaginate.totalPages)"> Next </a>
      <span ng-if="tkPaginate.isSelected(tkPaginate.totalPages)" class="tk-paginate-disabled"> Next </span>
    </nav>
  </div>
</div>
</script><script id="/angular_templates/directives/spectrum/dropdown.html" type="text/ng-template"><div class="spectrum-Dropdown"
     ng-model="spectrumDropdown.selectedValue"
     ng-change="spectrumDropdown.handleChange()"
     dropdown-base>
  <button class="spectrum-FieldButton spectrum-Dropdown-trigger"
          aria-haspopup="true"
          dropdown-base-button
          data-id="{{ spectrumDropdown.triggerButtonDataId }}">
    <span class="spectrum-Dropdown-label">
      {{ spectrumDropdown.getLabel() }}
    </span>

    <svg class="spectrum-Icon spectrum-UIIcon-ChevronDownMedium spectrum-Dropdown-icon" focusable="false" aria-hidden="true">
      <use xlink:href="#spectrum-css-icon-ChevronDownMedium"></use>
    </svg>
  </button>

  <div class="spectrum-Popover spectrum-Popover--bottom" dropdown-base-content>
    <ul class="spectrum-Menu" role="listbox">
      <li ng-if="spectrumDropdown.prioritizedItem" class="spectrum-Menu-item"
          role="option"
          tabindex="0"
          dropdown-base-item
          value="spectrumDropdown.prioritizedItem.value">
        <span class="spectrum-Menu-itemLabel">
          {{ spectrumDropdown.prioritizedItem.label }}
        </span>
      </li>
      <li ng-if="spectrumDropdown.prioritizedItem" class="spectrum-Menu-divider" role="separator"></li>
      <li class="spectrum-Menu-item"
          role="option"
          tabindex="0"
          ng-repeat="item in spectrumDropdown.menuItems()"
